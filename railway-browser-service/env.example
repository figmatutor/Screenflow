# Railway Browser Service 환경변수 예시

# 서버 설정
PORT=3000
NODE_ENV=production

# 보안 설정
BROWSER_SERVICE_API_KEY=your-secure-api-key-here
ALLOWED_ORIGINS=https://your-vercel-app.vercel.app,http://localhost:3000

# 브라우저 설정
MAX_BROWSERS=5
BROWSER_TIMEOUT=300000

# Redis 설정 (선택사항)
REDIS_URL=redis://localhost:6379

# 로깅 설정
LOG_LEVEL=info
